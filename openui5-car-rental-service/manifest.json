{
    "sap.app": {
      "id": "openui5-car-rental-service",
      "type": "application"
    },
    "sap.ui": {
      "technology": "UI5",
      "viewPath": "openui5-car-rental-service.view"
    },
    "sap.ui5": {
      "rootView": {
        "viewName": "openui5-car-rental-service.view.Main",
        "type": "XML"
      },
      "dependencies": {
        "libs": {
          "sap.m": {},
          "sap.ui.core": {}
        }
      },
      "routing":{
        "config":{
            "routerClass": "sap.m.routing.Router",
            "viewType": "XML",
            "viewPath": "openui5-car-rental-service.view",
            "controlId": "app", 
            "controlAggregation": "pages",
            "bypassed": {
                "target": "notFound"
            },
            "async": true
        },
        "routes": [
            {
                "pattern":"",
                "name":"Main",
                "target":"Main"
            },
            {
                "pattern":"logoff",
                "name":"Logoff",
                "target":"Logoff"
            },
            {
                "pattern":"login",
                "name":"Login",
                "target":"Login"
            },
            {
                "pattern":"new_car",
                "name":"NewCar",
                "target":"NewCar"
            },
            {
                "pattern":"edit_car",
                "name":"EditCar",
                "target":"EditCar"
            },
            {
                "pattern":"vehicle/{id]/view",
                "name": "VehicleView",
                "target": "VehicleView"
            }
        ],
        "targets":
        {
            "Main":
            {
                "viewName":"Main"
            },
            "Logoff":
            {
                "viewName":"LogoffPage"
            },
            "Login":
            {
                "viewName":"Login"
            },
            "NewCar":
            {
                "viewName":"NewCar"
            },
            "EditCar":
            {
                "viewName":"EditCar"
            },
            "VehicleView":
            {
                "viewName":"VehicleView"
            }
        }
        }
      }
    }
  
  